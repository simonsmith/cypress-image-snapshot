{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "types": ["node", "cypress"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "dist",
    "declarationMap": true,

    "outDir": "dist"
  },
  "include": ["src", "./custom.d.ts"]
}
